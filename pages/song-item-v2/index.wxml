<!--pages/song-item-v2/index.wxml-->
<view class="item" bindtap="handleItemClick">
    <view class="index">
        {{index}}
    </view>
    <view class="info">
    <view class="name">{{item.name}}</view>
    <view class="source">
    <image class="icon" src="/assets/images/icons/sq_icon.png" ></image>
    <text>{{item.ar[0].name || item.artists[0].name}} 
    <text wx:if=" {{item.alia[0]}}"> {{item.alia[0]}}</text>
    </text>
    </view>
    </view>
</view>